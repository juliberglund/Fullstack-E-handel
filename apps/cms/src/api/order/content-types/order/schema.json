{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "items": {
      "type": "json"
    },
    "total": {
      "type": "decimal"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Pending, Paid, Shipped"
      ]
    },
    "costumers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "orders"
    }
  }
}
